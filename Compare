#include <stdio.h>
//#include <string.h>

int main()
{
   char s1[5] = "abeja";
   char s2[5] = "Abeja";
   int i;

   printf( "s1=%s\t", s1 );
   printf( "s2=%s\n", s2 );

   i = strcmp( s1, s2 );
   printf( "s1 es " );
   if( i < 0 )  printf( "menor que ,i=%i",i );
   else if( i > 0 )  printf( "mayor que,i=%i",i);
   else  printf( "igual a" );
   printf( " s2\n" );

   return 0;
}
